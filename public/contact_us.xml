<t name="Contact Us" t-name="website.contactus">
    <t t-call="website.layout">
        <t t-set="logged_partner" t-value="request.env['website.visitor']._get_visitor_from_request().partner_id" />
        <t t-set="contactus_form_values" t-value="{                 'email_to': res_company.email,                 'name': request.params.get('name', ''),                 'phone': request.params.get('phone', ''),                 'email_from': request.params.get('email_from', ''),                 'company': request.params.get('company', ''),                 'subject': request.params.get('subject', ''),             }" />
        <span class="hidden" data-for="contactus_form" t-att-data-values="contactus_form_values" />
        <div id="wrap" class="oe_structure oe_empty">
            <section class="s_title parallax s_parallax_is_fixed bg-black-50 pt24 o_colored_level pb232" data-vcss="001" data-snippet="s_title" data-scroll-background-ratio="1" data-name="Title" style="background-image: none;">
                <span class="s_parallax_bg oe_img_bg o_bg_img_center" style="background-image: url(&quot;/web/image/2508-a584b048/Cueacao-2222222222.jpg&quot;); background-position: 50% 90.2778%;" data-original-id="2499" data-original-src="/web/image/2499-e140438a/Cueacao-2222222222.jpg" data-mimetype="image/jpeg" data-resize-width="1920" />
                <div class="o_we_bg_filter bg-black-50" />
                <div class="container">
                    <p style="text-align: center;">
                        <br />
                    </p>
                    <p style="text-align: center;">
                        <br />
                    </p>
                    <p style="text-align: center;">
                        <span style="font-weight: bolder;">WE ARE SMART-OBC</span>
                    </p>
                    <h1 style="text-align: center;">
                        <span style="font-weight: bolder;">Get in touch with us</span>
                    </h1>
                </div>
            </section>
            <section class="s_embed_code pt64 pb64 o_colored_level text-center" data-snippet="s_embed_code" data-name="Embed Code" style="background-image: none;">
                <div class="s_embed_code_embedded o_not_editable container">
                    <div class="contact-form">
                        <div class="contact-location">
                            <div>
                                <h2>Locations:</h2>
                                <br />
                                <br />
                                <p>We have offices at Curaçao, The Bahamas and the Dominican Republic, we deliver services in the whole Caribbean.</p>
                                <br />
                                <br />
                                <div>
                                    <p>+599 9 737 0727</p>
                                    <br />
                                    <br />
                                    <p>hola@smart-obc.com</p>
                                    <br />
                                    <br />
                                    <p>Head Office: Willemstad, Curaçao</p>
                                </div>
                                <div>
                                    <p>OUR SOCIAL PROFIELS</p>
                                    <ul>
                                        <li>F</li>
                                        <li>I</li>
                                        <li>L</li>
                                        <li>Y</li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="contact-us">
                            <div class="contact-names">
                                <div class="contact-firstname">
                                    <span>First Name</span>
                                    <input type="text" name="First Name" />
                                </div>
                                <div class="contact-lastname">
                                    <span>Last Name</span>
                                    <input type="text" name="Last Name" />
                                </div>
                            </div>
                            <div class="contact-email">
                                <span>Email</span>
                                <input type="email" name="Email" />
                            </div>
                            <div class="contact-subject">
                                <span>Subject</span>
                                <input type="text" name="subject " />
                            </div>
                            <div class="contact-message">
                                <span>Message</span>
                                <input type="text" name="Message" />
                            </div>
                        </div>
